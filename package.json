  "main": "index.js",
  "scripts": {
    "test": "karma start",
    "aot": "ngc -p tsconfig-aot.json",
    "clean": "rimraf build dist server/dist",
    "mv:html": "node ./config/mv-html.js",
    "build": "webpack --display-error-details",
    "build:sass": "node-sass -r build/ -o build/",
    "build:analyse": "webpack --display-error-details --env.analyse",
    "build:stage": "cross-env NODE_ENV=staging npm run build",
    "build:stage:analyse": "cross-env NODE_ENV=staging npm run build:analyse",
    "build:stage:test": "cross-env NODE_ENV=stage_test npm run build",
    "build:prod:analyse": "cross-env NODE_ENV=production npm run build:analyse",
    "build:prod:test": "cross-env NODE_ENV=prod_test npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "serve:stage": "npm run start --prefix ./server",
    "serve:prod": "npm run start NODE_ENV=production --prefix ./server",
    "local": "cross-env NODE_ENV=development webpack-dev-server",
    "local:hmr": "cross-env NODE_ENV=development webpack-dev-server --env.HMR",
    "deploy:dev": "npm-run-all clean build:stage mv:html",
    "dev:test": "npm-run-all clean build:stage:test mv:html",
    "deploy:test": "npm-run-all clean build:prod:test mv:html",
    "deploy": "npm-run-all clean build:prod mv:html"
  },
